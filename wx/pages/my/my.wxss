/* pages/my/my.wxss */
.container{
    width: 100%;
    box-sizing: border-box;
    padding: 0 13px;

}
.box{
    margin-bottom: 10px;
}
.title{
    font-size: 20px;
    color: #333;
    margin-bottom: 10px;
    text-align: center;
}
.body{
    line-height: 20px;
    color: #555;
    text-indent: 2em;
}
.body .telName{
  width: 35%;
}
.imgBox{
    display: flex;
    justify-content: space-around;
    flex-wrap: wrap;
}
.imgBox image{
    width: 40%;
    margin-bottom: 15px;
    border-radius: 5px;
}
.tel{
    width: 65%;
    margin: 0 auto;
    overflow: hidden;
}
.telNum{
    float: left;
}
.telName{
    float: left;
    text-indent: 0;
}
