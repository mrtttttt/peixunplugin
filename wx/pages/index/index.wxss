/**index.wxss**/
.search-btn {
  width: 100%;padding: 6px 12px;height: 40px;background: #F9F022;
  box-sizing: border-box;
}
.search-btn view{
  width: 100%;height: 100%;background: #000;background: #fff;text-align: center;color:#B2B2B2;font-size: 14px;
  line-height: 28px;border-radius: 3px;
}
.search-btn icon{
  line-height: normal;
}
.new-food{
  padding-top: 10px;margin-bottom: 20px;
}
.new-head{
  width: 100%;
  box-sizing: border-box;
  border-left: 8px solid #F9F022;padding-left: 10px;height: 20px;line-height: 20px;
  font-size: 16px;margin-bottom: 15px;
}
/* .new-scroll{width:1150px;overflow: hidden;height: 180px; } */
.new-scroll .new-scroll-list{
  width:100px;overflow: hidden;height: 180px;margin-right: 15px;
  display: inline-block;line-height: 40px;text-align: center;font-size: 14px;
}
.new-scroll .new-scroll-list:last-of-type{margin-right: 0;}
.new-scroll-list view{width: 100%;height:140px;border-radius: 5px;}
.foods-type{overflow: hidden;}
.food-list{
  width: 30%;height: 140px;line-height: 40px;text-align: center;float: left;
}
.food-list:nth-of-type(3n+2){
  margin: 0 5%;
}
.food-list view{width: 100%;height: 100px;border-radius: 5px;}

