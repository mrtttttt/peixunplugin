/* pages/typeManage/typeManage.wxss */
.container{
  width: 100%;
  height: 100%;
  background: #f0f0f0;
  padding: 0;
}
.ul{
  width: 100%;
  background: #fff;
  margin-bottom: 10px;
  padding: 0 15px;
  box-sizing: border-box;
}
.ul:last-of-type{
  margin-bottom: 0;
  text-align: center
}
.li{
  width: 100%;
  border-bottom: 1px solid #f0f0f0;
  line-height: 20px;
  padding: 15px 0;
}
.li icon{
  float:right;
  margin-right: 15px;
}
.li icon:first-of-type{
  margin-right: 0px;
}